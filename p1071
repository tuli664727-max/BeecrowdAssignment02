#include <iostream>
using namespace std;
 
int main() {
   int X, Y;
   cin >> X >> Y;
 
   int sum = 0;
 
   // Make sure we loop from the smaller to the larger number
   int start = min(X, Y);
   int end = max(X, Y);
 
   // Loop through numbers between start and end (excluding them)
   for (int i = start + 1; i < end; i++) {
       if (i % 2 != 0) { // Check if the number is odd
           sum += i;
       }
   }
 
   cout << sum << endl;
 
   return 0;
}
