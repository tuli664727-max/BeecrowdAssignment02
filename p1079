#include <iostream>
#include <iomanip>
using namespace std;
 
int main() {
   int N;
   cin >> N;
 
   for (int i = 0; i < N; i++) {
       double a, b, c;
       cin >> a >> b >> c;
 
       double weighted_average = (a * 2 + b * 3 + c * 5) / 10.0;
 
       cout << fixed << setprecision(1) << weighted_average << endl;
   }
 
   return 0;
}
