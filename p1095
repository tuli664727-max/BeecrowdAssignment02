#include <iostream>
using namespace std;
 
int main() {
   int I = 1;
   int J = 60;
 
   while (J >= 0) {
       cout << "I=" << I << " J=" << J << endl;
       I += 3;
       J -= 5;
   }
 
   return 0;
}
