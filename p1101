#include <iostream>
using namespace std;
 
int main() {
   int M, N;
 
   while (true) {
       cin >> M >> N;
       if (M <= 0 || N <= 0) break; // Stop if any number <= 0
 
       int start = min(M, N);
       int end = max(M, N);
       int sum = 0;
 
       for (int i = start; i <= end; i++) {
           cout << i << " ";
           sum += i;
       }
       cout << "Sum=" << sum << endl;
   }
 
   return 0;
}
 
