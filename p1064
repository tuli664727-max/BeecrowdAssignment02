#include <iomanip>
using namespace std;
 
int main() {
   double num, sum = 0.0;
   int count = 0;
 
   for (int i = 0; i < 6; i++) {
       cin >> num;
       if (num > 0) {
           sum += num;
           count++;
       }
   }
 
   cout << count << " valores positivos" << endl;
   cout << fixed << setprecision(1) << (sum / count) << endl;
 
   return 0;
}
 
