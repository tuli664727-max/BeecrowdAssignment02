#include <iostream>
using namespace std;
 
int main() {
   int number, maxNumber = 0, position = 0;
 
   for (int i = 1; i <= 100; i++) {
       cin >> number;
       if (number > maxNumber) {
           maxNumber = number;
           position = i;
       }
   }
 
   cout << maxNumber << endl;
   cout << position << endl;
 
   return 0;
}
