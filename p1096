#include <iostream>
using namespace std;
 
int main() {
   for (int I = 1; I <= 9; I += 2) {   // I = 1, 3, 5, 7, 9
       for (int J = 7; J >= 5; J--) { // J = 7, 6, 5
           cout << "I=" << I << " J=" << J << endl;
       }
   }
   return 0;
}
1097
#include <iostream>
using namespace std;
 
int main() {
   for (int I = 1; I <= 9; I += 2) {           // I = 1, 3, 5, 7, 9
       for (int J = I + 6; J >= I + 4; J--) {  // J = I+6, I+5, I+4
           cout << "I=" << I << " J=" << J << endl;
       }
   }
   return 0;
}
